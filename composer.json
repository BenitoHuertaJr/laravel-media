{
    "name": "iamx/laravel-media",
    "description": "This a simple way to use media files in laravel",
    "require": {
        "php": "^7.2.5|^8.0",
        "intervention/image": "^2.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Benito Huerta",
            "email": "huertajosebenito@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "iamx\\Media\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "iamx\\Media\\Providers\\MediaServiceProvider"
            ]
        }
    }
}
